{
  "cSpell.words": [
    "browserslistrc",
    "buble",
    "compat",
    "conventionalcommits",
    "cssnano",
    "customizer",
    "czrc",
    "hpagent",
    "kityandhero",
    "lintstagedrc",
    "ngrx",
    "noninteractive",
    "npmrc",
    "packagejson",
    "pmmmwh",
    "postbuild",
    "postchange",
    "postclean",
    "postconfig",
    "postcz",
    "postdev",
    "postlint",
    "postncu",
    "postpublish",
    "postz",
    "precommit",
    "precz",
    "prelerna",
    "prencu",
    "prereinstall",
    "prez",
    "prismjs",
    "pxtransform",
    "Redirector",
    "reduxjs",
    "regexes",
    "sleepms",
    "stylelintignore",
    "svgr",
    "unmodel"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "eslint.packageManager": "pnpm",
  "git.ignoreLimitWarning": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/coverage": true,
    "**/docs": true,
    "**/pnpm-lock.yaml": true,
    "**/es": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  }
}
