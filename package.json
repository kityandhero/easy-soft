{
  "name": "lerna-easy-soft-utility",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:all": "npm run build:core-dva-compression && npm run build:core-utility-compression",
    "build:core-dva-compression": "cd packages/easy-soft-dva && npm run build:rollup",
    "build:core-dva-skip-compression": "cd packages/easy-soft-dva && npm run build:rollup:skipCompression",
    "build:core-utility-compression": "cd packages/easy-soft-utility && npm run build:rollup",
    "build:core-utility-skip-compression": "cd packages/easy-soft-utility && npm run build:rollup:skipCompression",
    "change-nrm-local": "nrm use local && npm run sleep:change-nrm",
    "change-nrm-npm": "nrm use npm && npm run sleep:change-nrm",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "preclean:all": "rimraf ./yarn-error.log && rimraf ./yarn.lock && rimraf ./package-lock.json",
    "clean:all": "lerna clean -y",
    "postclean:all": "npx rimraf ./package-lock.json && npm run clean:core-cli && npm run clean:core-utility && npm run clean:core-dva && npx rimraf ./node_modules",
    "clean:core-cli": "cd packages/easy-soft-cli && npm run clean-file",
    "clean:core-dva": "cd packages/easy-soft-dva && npm run clean-file",
    "clean:core-utility": "cd packages/easy-soft-utility && npm run clean-file",
    "commitlint": "npx commitlint --edit",
    "config:env": "node ./develop/assists/env.init.js",
    "postconfig:env": "npm run prettier:package.json:all",
    "precz": "npm run update:cz:flag && git stage -A",
    "cz": "cz",
    "postcz": "git push",
    "postinstall": "npm run config:env",
    "prelerna:bootstrap": "npm run change-nrm-local",
    "lerna:bootstrap": "npm run clean:all && husky install && git pull && pnpm install",
    "lerna:publish": "lerna publish --yes",
    "lint-staged": "npx lint-staged",
    "lint:all:fix": "npm run lint:core-cli:fix && npm run lint:core-utility:fix && npm run lint:core-dva:fix",
    "lint:core-cli:fix": "cd packages/easy-soft-cli && ncu run lint:fix",
    "lint:core-dva:fix": "cd packages/easy-soft-dva && ncu run lint:fix",
    "lint:core-utility:fix": "cd packages/easy-soft-example-app && ncu run lint:fix",
    "lint:example:fix": "cd packages/easy-soft-utility && ncu run lint:fix",
    "lint:prettier": "npx prettier --check \"**/*\" --end-of-line auto",
    "lint:staged": "npx lint-staged",
    "lint:style": "npx stylelint --fix \"src/**/*.less\"",
    "prencu:all": "npm run change-nrm-local",
    "ncu:all": "ncu && npm run ncu:core-cli && npm run ncu:core-dva && npm run ncu:core-utility && npm run ncu:example",
    "ncu:core-cli": "cd packages/easy-soft-cli && ncu",
    "ncu:core-dva": "cd packages/easy-soft-dva && ncu",
    "ncu:core-utility": "cd packages/easy-soft-utility && ncu",
    "ncu:example": "cd packages/easy-soft-example-app && ncu",
    "prencu:u:all": "npm run change-nrm-local",
    "ncu:u:all": "ncu -u && npm run ncu:u:core-cli && npm run ncu:u:core-dva && npm run ncu:u:core-utility && npm run ncu:u:example",
    "postncu:u:all": "pnpm install",
    "ncu:u:core-cli": "cd packages/easy-soft-cli && ncu -u",
    "ncu:u:core-dva": "cd packages/easy-soft-dva && ncu -u",
    "ncu:u:core-utility": "cd packages/easy-soft-utility && ncu -u",
    "ncu:u:example": "cd packages/easy-soft-example-app && ncu -u",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prettier": "npx prettier -c --write \"**/*\"",
    "prettier:format:all": "npx prettier --write .",
    "prettier:format:change": "npx prettier --cache --write .",
    "prettier:package.json:all": "npx prettier --write ./**/package.json",
    "prettier:package.json:current": "npx prettier --write ./package.json",
    "prepublish-npm-all": "npm run change-nrm-npm",
    "publish-npm-all": "nrm ls && npm run publish:npm-all",
    "postpublish-npm-all": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-cli": "npm run change-nrm-npm",
    "publish-npm-cli": "nrm current && npm run publish:npm-cli",
    "postpublish-npm-cli": "npm run change-nrm-local && nrm current",
    "prepublish-npm-dva": "npm run change-nrm-npm",
    "publish-npm-dva": "nrm current && npm run publish:npm-dva",
    "postpublish-npm-dva": "npm run change-nrm-local && nrm current",
    "prepublish-npm-utility": "npm run change-nrm-npm",
    "publish-npm-utility": "nrm current && npm run publish:npm-utility",
    "postpublish-npm-utility": "npm run change-nrm-local && nrm current",
    "prepublish:build": "pnpm install && npm run cz && npm run build:all",
    "publish:build": "npm run publish:lerna",
    "prepublish:lerna": "npm run change-nrm-npm",
    "publish:lerna": "lerna updated && npm run lerna:publish",
    "postpublish:lerna": "npm run change-nrm-local && npm run publish-npm-all",
    "publish:npm-all": "npm run publish:npm-utility && npm run publish:npm-dva && npm run publish:npm-cli",
    "publish:npm-cli": "cd packages/easy-soft-cli && npm publish",
    "publish:npm-dva": "cd packages/easy-soft-dva && npm publish",
    "publish:npm-utility": "cd packages/easy-soft-utility && npm publish",
    "reinstall": "npm run lerna:bootstrap",
    "show:info": "echo node version && node --version && echo npm version && npm --version && echo ------------ && npx lerna ls -a -l",
    "show:package": "npx lerna ls -a -l",
    "sleep:change-nrm": "node ./develop/assists/sleep.js",
    "start:example:h5": "cd packages/easy-soft-example-app && npm run dev:h5",
    "update:cz:flag": "node ./develop/assists/cz.flag.refresh.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/config-lerna-scopes": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^6.5.1",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-taro": "^3.5.12",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "rimraf": "^4.1.2",
    "rollup": "^3.12.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "shelljs": "^0.8.5",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  }
}
