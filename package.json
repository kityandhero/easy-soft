{
  "name": "lerna-easy-soft-utility",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:core-all": "npm run build:core-utility-compression && npm run build:core-dva-compression",
    "build:core-all-skip": "npm run build:core-utility-compression-skip && npm run build:core-dva-compression-skip",
    "build:core-dva-compression": "cd packages/easy-soft-dva && npm run build:rollup",
    "build:core-dva-compression-skip": "cd packages/easy-soft-dva && npm run build:rollup:skipCompression",
    "build:core-utility-compression": "cd packages/easy-soft-utility && npm run build:rollup",
    "build:core-utility-compression-skip": "cd packages/easy-soft-utility && npm run build:rollup:skipCompression",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "commitlint": "npx commitlint --edit",
    "postinstall": "npm run z:initial:environment",
    "precommit": "npm run z:lint:staged:quiet",
    "prepare": "npm run z:husky:install && echo do other prepare work with here",
    "prepublish-npm-dva": "npm run z:change-nrm-npm",
    "publish-npm-dva": "nrm current && npm run publish:npm-dva",
    "postpublish-npm-dva": "npm run z:change-nrm-local && nrm current",
    "prepublish-npm-utility": "npm run z:change-nrm-npm",
    "publish-npm-utility": "nrm current && npm run publish:npm-utility",
    "postpublish-npm-utility": "npm run z:change-nrm-local && nrm current",
    "publish:npm-dva": "cd packages/easy-soft-dva && npm publish --registry https://registry.npmjs.org/",
    "publish:npm-utility": "cd packages/easy-soft-utility && npm publish --registry https://registry.npmjs.org/",
    "start:example:h5": "cd packages/easy-soft-example-app && npm run dev:h5",
    "test:easy-soft-dva": "cd packages/easy-soft-dva && npm run z:test",
    "test:easy-soft-example-app": "cd packages/easy-soft-example-app && npm run z:test",
    "test:easy-soft-utility": "cd packages/easy-soft-utility && npm run z:test",
    "z:auto:adjust:file:all": "npm run z:auto:adjust:file:easy-soft-dva && npm run z:auto:adjust:file:easy-soft-example-app && npm run z:auto:adjust:file:easy-soft-utility",
    "z:auto:adjust:file:easy-soft-dva": "cd packages/easy-soft-dva && npm run z:auto:adjust:file",
    "z:auto:adjust:file:easy-soft-example-app": "cd packages/easy-soft-example-app && npm run z:auto:adjust:file",
    "z:auto:adjust:file:easy-soft-utility": "cd packages/easy-soft-utility && npm run z:auto:adjust:file",
    "z:build:all": "npm run build:core-all",
    "z:change:npm:registry:local": "nrm use local",
    "z:change:npm:registry:npm": "nrm use npm",
    "z:check:all-package-version": "npx easy-soft-develop check-all-package-version",
    "z:clean": "node ./develop/assists/clean.js",
    "z:commit:refresh": "npx easy-soft-develop commit-refresh",
    "z:create:assist-scripts": "npx easy-soft-develop create-assist-scripts",
    "prez:cz": "npm run z:auto:adjust:file:all && npm run z:prettier:format:change && npm run z:commit:refresh && git stage -A",
    "z:cz": "cz",
    "postz:cz": "git push && npm run z:test",
    "z:husky:install": "npx husky install",
    "prez:initial:environment": "npm run z:create:assist-scripts",
    "z:initial:environment": "node ./develop/assists/initial.environment.js",
    "prez:install": "npm run z:change:npm:registry:local",
    "z:install": "pnpm install",
    "prez:install.global.develop.dependence": "npm run z:change:npm:registry:local",
    "z:install.global.develop.dependence": "node ./develop/assists/install.global.develop.dependence",
    "postz:install.global.develop.dependence": "npm run z:install",
    "z:lerna:bootstrap": "npm run z:clean && npm run z:husky:install && git pull && npm run z:install",
    "z:lerna:publish": "lerna publish --yes",
    "z:lint:staged": "npx lint-staged",
    "z:lint:staged:quiet": "npx lint-staged --quiet",
    "z:prettier:format:all": "npx prettier --write .",
    "z:prettier:format:change": "npx prettier --cache --write . && npx easy-soft-develop prompt --message \"format changed files complete\" --type success --blankLine",
    "z:prettier:package.json:all": "npx prettier --write ./**/package.json",
    "z:prettier:package.json:current": "npx prettier --write ./package.json",
    "prez:publish:build": "npm run z:install && npm run z:cz && npm run z:build:all",
    "z:publish:build": "npm run z:publish:lerna",
    "prez:publish:lerna": "npm run z:change:npm:registry:npm",
    "z:publish:lerna": "lerna updated && npm run z:lerna:publish",
    "postz:publish:lerna": "npm run z:change:npm:registry:local && npm run z:publish:npm-all",
    "z:publish:npm-all": "npx easy-soft-develop publish --packages easy-soft-dva,easy-soft-utility",
    "z:reinstall": "npm run z:lerna:bootstrap",
    "z:show:info": "echo node version && node --version && echo npm version && npm --version && echo ------------ && npx lerna ls -a -l",
    "z:show:package": "npx lerna ls -a -l",
    "z:sleep": "npx easy-soft-develop sleep --second 2 --showInfo false",
    "z:test": "npm run test:easy-soft-dva && npm run test:easy-soft-example-app && npm run test:easy-soft-utility",
    "z:update:all-package-version": "npx easy-soft-develop update-all-package-version",
    "postz:update:all-package-version": "npm run z:install",
    "z:update:package-from-package": "node ./develop/assists/update-package-from-package.js",
    "z:update:special-package-version": "node ./develop/assists/package.update.special.version.js",
    "postz:update:special-package-version": "npm run z:install"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-external-helpers": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/runtime": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/config-lerna-scopes": "^17.6.3",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-buble": "^1.0.2",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-url": "^8.0.1",
    "@svgr/rollup": "^8.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@typescript-eslint/parser": "^5.59.9",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^6.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "dayjs": "^1.11.8",
    "documentation": "^14.0.2",
    "easy-soft-develop": "^2.1.20",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lerna": "^7.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.25.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "shelljs": "^0.8.5",
    "stylelint": "^15.7.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^33.0.0",
    "terminal-kit": "^3.0.0"
  }
}
