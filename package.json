{
  "name": "lerna-easy-soft-utility",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "kityandhero <kityandhero@126.com>",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build:all": "npm run build:core-dva && npm run build:core-utility",
    "build:core-dva": "cd packages/easy-soft-dva && npm run build",
    "build:core-dva-skipCompression": "cd packages/easy-soft-dva && npm run build:skipCompression",
    "build:core-utility": "cd packages/easy-soft-utility && npm run build",
    "build:core-utility-skipCompression": "cd packages/easy-soft-utility && npm run build:skipCompression",
    "change-nrm-local": "echo change nrm use npm && nrm use npm && npx sleepms 2000",
    "change-nrm-npm": "echo change nrm use npm && nrm use npm && npx sleepms 2000",
    "changelog": "lerna version --conventional-commits --no-push --no-git-tag-version",
    "preclean:all": "rimraf ./yarn-error.log && rimraf ./yarn.lock",
    "clean:all": "lerna clean -y",
    "postclean:all": "rimraf ./package-lock.json && npm run clean:core-cli && npm run clean:core-utility && npm run clean:core-dva && npm run clean:rollup && rimraf ./node_modules",
    "clean:core-cli": "cd packages/easy-soft-cli && npm run clean-file",
    "clean:core-dva": "cd packages/easy-soft-dva && npm run clean-file",
    "clean:core-utility": "cd packages/easy-soft-utility && npm run clean-file",
    "clean:rollup": "cd packages/easy-soft-rollup && npm run clean-file",
    "commitlint": "npx commitlint --edit",
    "precz": "npm run update:cz:flag && git stage -A",
    "cz": "cz",
    "postcz": "git push",
    "lerna": "lerna",
    "lerna:b": "husky install && git pull && pnpm install",
    "lerna:build": "lerna run --stream --sort build",
    "lerna:create": "lerna create",
    "lerna:publish": "lerna publish --yes",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "npx eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:all:fix": "npm run lint:core-cli:fix && npm run lint:core-utility:fix && npm run lint:core-dva:fix",
    "lint:core-cli:fix": "cd packages/easy-soft-cli && ncu run lint:fix",
    "lint:core-dva:fix": "cd packages/easy-soft-dva && ncu run lint:fix",
    "lint:core-utility:fix": "cd packages/easy-soft-example-app && ncu run lint:fix",
    "lint:example:fix": "cd packages/easy-soft-utility && ncu run lint:fix",
    "lint:js": "npx eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "npx prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "npx stylelint --fix \"src/**/*.less\"",
    "ncu:all": "ncu && npm run ncu:core-rollup && npm run ncu:core-cli && npm run ncu:core-dva && npm run ncu:core-utility && npm run ncu:example",
    "ncu:core-cli": "cd packages/easy-soft-cli && ncu",
    "ncu:core-dva": "cd packages/easy-soft-dva && ncu",
    "ncu:core-rollup": "cd packages/easy-soft-rollup && ncu",
    "ncu:core-utility": "cd packages/easy-soft-utility && ncu",
    "ncu:example": "cd packages/easy-soft-example-app && ncu",
    "ncu:u:all": "ncu -u && npm run ncu:u:core-rollup && npm run ncu:u:core-cli && npm run ncu:u:core-dva && npm run ncu:u:core-utility && npm run ncu:u:example",
    "ncu:u:core-cli": "cd packages/easy-soft-cli && ncu -u",
    "ncu:u:core-dva": "cd packages/easy-soft-dva && ncu -u",
    "ncu:u:core-rollup": "cd packages/easy-soft-rollup && ncu -u",
    "ncu:u:core-utility": "cd packages/easy-soft-utility && ncu -u",
    "ncu:u:example": "cd packages/easy-soft-example-app && ncu -u",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prettier": "npx prettier -c --write \"**/*\"",
    "prepublish-npm-all": "npm run change-nrm-npm",
    "publish-npm-all": "nrm ls && npm run publish:npm-all && nrm use local && npx sleepms 2000",
    "postpublish-npm-all": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-cli": "npm run change-nrm-npm",
    "publish-npm-cli": "nrm ls && npm run publish:npm-cli && nrm use local && npx sleepms 2000",
    "postpublish-npm-cli": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-dva": "npm run change-nrm-npm",
    "publish-npm-dva": "nrm ls && npm run publish:npm-dva && nrm use local && npx sleepms 2000",
    "postpublish-npm-dva": "npm run change-nrm-local && nrm ls",
    "prepublish-npm-utility": "npm run change-nrm-npm",
    "publish-npm-utility": "nrm ls && npm run publish:npm-utility && nrm use local && npx sleepms 2000",
    "postpublish-npm-utility": "npm run change-nrm-local && nrm ls",
    "prepublish:build": "npm run cz && npm run build:all",
    "publish:build": "npm run publish:lerna",
    "postpublish:build": "npm run publish-npm-all",
    "prepublish:build-skip": "npm run cz",
    "publish:build-skip": "npm run publish:lerna",
    "postpublish:build-skip": "npm run publish-npm-all",
    "publish:lerna": "lerna updated && lerna publish --yes",
    "publish:npm-all": "npm run publish:npm-utility && npm run publish:npm-dva && npm run publish:npm-cli",
    "publish:npm-cli": "cd packages/easy-soft-cli && npm publish",
    "publish:npm-dva": "cd packages/easy-soft-dva && npm publish",
    "publish:npm-utility": "cd packages/easy-soft-utility && npm publish",
    "reinstall": "npm run clean:all && npm run lerna:b",
    "show:all": "lerna ls -a -l",
    "start:example:h5": "cd packages/easy-soft-example-app && npm run dev:h5",
    "update:cz:flag": "node cz.flag.refresh.js"
  },
  "dependencies": {
    "rollup-plugin-copy": "^3.4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.20.7",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@commitlint/config-lerna-scopes": "^17.4.2",
    "@commitlint/cz-commitlint": "^17.4.2",
    "commitizen": "^4.2.6",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-taro": "^3.5.11",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "lerna": "^6.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "sleep-ms": "^2.0.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0"
  }
}
